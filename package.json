{
    "name": "angular-quickstart",
    "version": "0.1.0",
    "scripts": {
        "develop": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
        "lite": "lite-server",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "typings": "typings",
        "ngc": "./node_modules/.bin/ngc -p ./tsconfig-aot.json",
        "rollup": "./node_modules/.bin/rollup -c ./rollup.js",
        "build": "./node_modules/.bin/ngc -p ./tsconfig-aot.json && ./node_modules/.bin/rollup -c ./rollup.js",
        "start": "node app.js"
    },
    "dependencies": {
        "@angular/common": "~2.0.1",
        "@angular/compiler": "~2.0.1",
        "@angular/compiler-cli": "^0.6.3",
        "@angular/core": "~2.0.1",
        "@angular/forms": "~2.0.1",
        "@angular/http": "~2.0.1",
        "@angular/platform-browser": "~2.0.1",
        "@angular/platform-browser-dynamic": "~2.0.1",
        "@angular/platform-server": "^2.0.1",
        "@angular/router": "~3.0.1",
        "@angular/upgrade": "~2.0.1",
        "angular-in-memory-web-api": "~0.1.1",
        "bootstrap": "^3.3.7",
        "compression": "^1.6.2",
        "core-js": "^2.4.1",
        "express": "^4.14.0",
        "reflect-metadata": "^0.1.8",
        "rxjs": "5.0.0-beta.12",
        "systemjs": "0.19.39",
        "zone.js": "^0.6.25"
    },
    "devDependencies": {
        "concurrently": "^3.0.0",
        "lite-server": "^2.2.2",
        "rollup": "^0.36.1",
        "rollup-plugin-commonjs": "^5.0.4",
        "rollup-plugin-node-resolve": "^2.0.0",
        "rollup-plugin-uglify": "^1.0.1",
        "typescript": "^2.0.3",
        "typings": "^1.4.0"
    },
    "engines": {
        "node": "6.7.0"
    }
}
